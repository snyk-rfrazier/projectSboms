{
    "compilerOptions": {
      "esModuleInterop": true,
      "exactOptionalPropertyTypes": true,
      "isolatedModules": true,
      "lib": [
        "esnext"
      ],
      "module": "esnext",
      "moduleResolution": "node",
      "noUncheckedIndexedAccess": true,
      "outDir": "dist",
      "strict": true,
      "target": "esnext",
      "useUnknownInCatchVariables": true
    }
  }